er, kvalitets og kostnadsstyring."}],"assessment":[{"code":"S\u00d8","codeName":"Skriftlig\/Arbeider","combinationCode":"S\u00d8-1","combinationName":"Skriftlig eksamen","firstExecutionYear":2012,"firstExecutionTerm":"Autumn","realExecutionYear":2012,"realExecutionTerm":"Autumn","date":"2012-12-19","appearanceTime":"09:00","statusCode":"ORD","statusName":"Ordin\u00e6r eksamen","gradeWeightingNumerator":60,"gradeWeightingDenominator":100,"duration":4,"deadlineBackOut":"2012-11-15","assessmentFormCode":"S","assessmentFormDescription":"Skriftlig eksamen","assessmentTimeCode":"12","partNr":1,"registrationDeadline":"2012-09-15","gradeRuleScheme":"30","gradeRuleSchemeName":"Bokstavkarakterer","gradeRuleUnit":"30","gradeRuleUnitName":"Bokstavkarakterer","examinationSupport":[{"code":"D","name":"Ingen trykte eller h\u00e5ndskrevne hjelpemidler tillatt. Bestemt, enkel kalkulator tillatt."}]},{"code":"S\u00d8","codeName":"Skriftlig\/Arbeider","combinationCode":"S\u00d8-2","combinationName":"Arbeider","firstExecutionYear":2012,"firstExecutionTerm":"Autumn","realExecutionYear":2012,"realExecutionTerm":"Autumn","statusCode":"ORD","statusName":"Ordin\u00e6r eksamen","gradeWeightingNumerator":40,"gradeWeightingDenominator":100,"duration":0,"deadlineBackOut":"2012-11-15","assessmentFormCode":"B","assessmentFormDescription":"Arbeider","assessmentTimeCode":"12","partNr":2,"registrationDeadline":"2012-09-15","gradeRuleScheme":"30","gradeRuleSchemeName":"Bokstavkarakterer","gradeRuleUnit":"30","gradeRuleUnitName":"Bokstavkarakterer"}],"educationLanguage":[{"code":"ENGELSK","name":"Engelsk","norwegianName":"Engelsk","englishName":"English"}],"educationalRole":[{"code":"SubjectLecturer","name":"Fagl\u00e6rer","term":"Autumn","personId":527935,"person":{"personId":527935,"dateOfBirth":null,"gender":"Male","firstName":"Henry Sigvart","lastName":"Valberg","displayName":"Henry Sigvart Valberg","email":"henry.valberg@ntnu.no","publicationStatus":"NotReserved","username":"henryv","officeAddres