rasjon p\u00e5 mangfoldigheter. \r\n"},{"code":"ANBFORK","name":"Anbefalte forkunnskaper","text":"Emnet bygger p\u00e5 TMA4190 Mangfoldigheter. Det er videre en fordel \u00e5 ha hatt noe analyse utover grunnkursene."},{"code":"FORK","name":"Forkunnskapskrav"}],"assessment":[{"code":"M","codeName":"Muntlig","combinationCode":"M1","combinationName":"Muntlig","firstExecutionYear":2012,"firstExecutionTerm":"Autumn","realExecutionYear":2012,"realExecutionTerm":"Autumn","statusCode":"ORD","statusName":"Ordin\u00e6r eksamen","gradeWeightingNumerator":100,"gradeWeightingDenominator":100,"duration":0,"deadlineBackOut":"2012-11-15","assessmentFormCode":"M","assessmentFormDescription":"Muntlig eksamen","assessmentTimeCode":"12","partNr":1,"registrationDeadline":"2012-09-15","gradeRuleScheme":"30","gradeRuleSchemeName":"Bokstavkarakterer","gradeRuleUnit":"30","gradeRuleUnitName":"Bokstavkarakterer","examinationSupport":[{"code":"D","name":"Ingen trykte eller h\u00e5ndskrevne hjelpemidler tillatt. Bestemt, enkel kalkulator tillatt."}]}],"educationLanguage":[{"code":"ENGELSK","name":"Engelsk","norwegianName":"Engelsk","englishName":"English"}],"educationalRole":[{"code":"Coordinator","name":"Koordinator","term":"Autumn","personId":526027,"person":{"personId":526027,"dateOfBirth":null,"gender":"Male","firstName":"Nils A.","lastName":"Baas","displayName":"Nils A. Baas","email":"nils.baas@math.ntnu.no","publicationStatus":"NotReserved","username":"baas","employee":true,"affiliated":false,"student":false}}],"educationTerm":[{"termNum":1,"startTerm":"Autumn","termApplies":"Autumn","year":2012,"yearIncrement":0,"inDepthStudyHours":8,"lectureHours":4,"registrationDeadline":"2012-09-15","numOfRegistratedStudents":6}],"subjectArea":[{"code":"MATEMATIKK","name":"Matematikk","norwegianName":"Matematikk","newNorwegianName":"Matematikk","englishName":"Mathematics"}],"creditReduction":[{"courseCode":"MNFMA317","weightRuleCode":1976,"creditsReduction":7.5,"creditsTypeReduction":"SP","ruleTypeCo