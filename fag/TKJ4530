uttet kurs skal studenten kunne:\r\n- Gj\u00f8re rede for det teoretiske grunnlag for prosjektet.\r\n- Gj\u00f8re rede for det praktiske arbeid og sette dette i relasjon til den teoretiske beskrivelse.\r\n- Gi en oversikt over sentral litteratur.\r\n- Gj\u00f8re rede for resultatene og sette disse i relasjon til \"state of the art\".\r\n"}],"assessment":[{"code":"\u00d8","codeName":"Arbeider","combinationCode":"\u00d8","combinationName":"Arbeider","firstExecutionYear":2012,"firstExecutionTerm":"Autumn","realExecutionYear":2012,"realExecutionTerm":"Autumn","statusCode":"ORD","statusName":"Ordin\u00e6r eksamen","gradeWeightingNumerator":100,"gradeWeightingDenominator":100,"duration":0,"deadlineBackOut":"2012-11-15","assessmentFormCode":"B","assessmentFormDescription":"Arbeider","assessmentTimeCode":"12","partNr":1,"registrationDeadline":"2012-09-15","gradeRuleScheme":"30","gradeRuleSchemeName":"Bokstavkarakterer","gradeRuleUnit":"30","gradeRuleUnitName":"Bokstavkarakterer"}],"educationLanguage":[{"code":"ENGELSK","name":"Engelsk","norwegianName":"Engelsk","englishName":"English"}],"educationalRole":[{"code":"Coordinator","name":"Koordinator","term":"Autumn","personId":520752,"person":{"personId":520752,"dateOfBirth":null,"gender":"Male","firstName":"Henrik","lastName":"Koch","displayName":"Henrik Koch","email":"henrik.koch@ntnu.no","publicationStatus":"NotReserved","username":"henrikk","officeAddress":"Realfagbygget*D3-109, H\u00f8gskoleringen 5","phone":"73594165","employee":true,"affiliated":false,"student":false}}],"educationTerm":[{"termNum":1,"startTerm":"Autumn","termApplies":"Autumn","year":2012,"yearIncrement":0,"inDepthStudyHours":24,"registrationDeadline":"2012-09-15","numOfRegistratedStudents":0}],"subjectArea":[{"code":"KJEMI","name":"Kjemi","norwegianName":"Kjemi","newNorwegianName":"Kjemi","englishName":"Chemistry"},{"code":"NANOTEKN","name":"Nanoteknologi","norwegianName":"Nanoteknologi","newNorwegianName":"Nanoteknologi","englishName":"Nanotechnology"}