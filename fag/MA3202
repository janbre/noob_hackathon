heter. Studenten kan beskrive Galoisgruppen til en gitt kroppsutvidelse, og finne korrespondanser mellom undergrupper og mellomkropper."}],"assessment":[{"code":"S","codeName":"Skriftlig","combinationCode":"S","combinationName":"Skriftlig","firstExecutionYear":2013,"firstExecutionTerm":"Spring","realExecutionYear":2013,"realExecutionTerm":"Spring","date":"2013-05-16","appearanceTime":"09:00","statusCode":"ORD","statusName":"Ordin\u00e6r eksamen","gradeWeightingNumerator":100,"gradeWeightingDenominator":100,"duration":4,"deadlineBackOut":"2013-04-30","assessmentFormCode":"S","assessmentFormDescription":"Skriftlig eksamen","assessmentTimeCode":"06","partNr":1,"registrationDeadline":"2013-02-15","gradeRuleScheme":"30","gradeRuleSchemeName":"Bokstavkarakterer","gradeRuleUnit":"30","gradeRuleUnitName":"Bokstavkarakterer","examinationSupport":[{"code":"D","name":"Ingen trykte eller h\u00e5ndskrevne hjelpemidler tillatt. Bestemt, enkel kalkulator tillatt."}]}],"educationLanguage":[{"code":"ENGELSK","name":"Engelsk","norwegianName":"Engelsk","englishName":"English"}],"educationalRole":[{"code":"Coordinator","name":"Koordinator","term":"Spring","personId":500622,"person":{"personId":500622,"dateOfBirth":null,"gender":"Female","firstName":"Idun","lastName":"Reiten","displayName":"Idun Reiten","email":"idun.reiten@math.ntnu.no","publicationStatus":"NotDefined","username":"idunr","officeAddress":"Sentralbygg II*838, Alfred Getz vei 1","phone":"73591742","mobilePhone":"41211821","employee":false,"affiliated":true,"student":false}}],"educationTerm":[{"termNum":1,"startTerm":"Spring","termApplies":"Spring","year":2013,"yearIncrement":0,"inDepthStudyHours":8,"lectureHours":4,"registrationDeadline":"2013-02-15","numOfRegistratedStudents":1}],"subjectArea":[{"code":"MATEMATIKK","name":"Matematikk","norwegianName":"Matematikk","newNorwegianName":"Matematikk","englishName":"Mathematics"}],"creditReduction":[{"courseCode":"MNFMA319","weightRuleCode":1972,"creditsReduction":7.5,"creditsT